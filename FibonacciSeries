package week1.day2;

public class FibonacciSeries {

	// Karthick Manickam - This assignment helped understanding for loop and assigning variables inside & outside loop
	 
		public static void main(String[] args) {

		int range = 8; int firstnum = 0; int secondnum = 1; int sum = 0; 
		System.out.println(firstnum);
		System.out.println(secondnum);
		for (int i=1; i<=range; i++) {
			sum = firstnum + secondnum; 
			firstnum = secondnum;
			secondnum = sum;
			System.out.println(sum);
		}
					
	}

}
