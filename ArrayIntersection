package week1.day2;

public class FindIntersectionArray {

	public static void main(String[] args) {
		int array1[] = { 1, 24, 4, 4, 6, 7 };
		int array2[] = { 1, 2, 4, 4, 9, 6 };

		for (int i = 0; i < array1.length; i++) {
			
				if (array1[i] == array2[i]) {
					System.out.println(array1[i]);
				}

			
		}
	}

}
